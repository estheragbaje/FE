exports[`<NewPassword /> renders the NewPassword component correctly 1`] = `
<styled.div>
  <form
    action="#!"
    className="text-center p-5"
    onSubmit={[Function]}
  >
    <p
      className="h4 mb-4 f-1"
    >
      Enter New Password
    </p>
    <TextInput
      id="newPassword"
      name="newPassword"
      onChange={[Function]}
      required={true}
      title="Password"
      type="password"
      value=""
    />
    <TextInput
      id="password"
      name="newPasswordConfirm"
      onChange={[Function]}
      required={true}
      title="Confirm Password"
      type="password"
      value=""
    />
    <styled.button
      className="btn btn-block my-4"
      type="submit"
    >
      Update Password
    </styled.button>
  </form>
</styled.div>
`;

exports[`<NewPassword /> should redirect if isSignedin is true 1`] = `
<Redirect
  to="/login"
/>
`;
