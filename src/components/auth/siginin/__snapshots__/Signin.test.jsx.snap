exports[`<Signin /> renders the Signin component correctly 1`] = `
<styled.div>
  <form
    className="text-center  p-4"
    id="signinForm"
    onSubmit={[Function]}
  >
    <p
      className="h4 mb-4 f-1"
    >
      Sign In
    </p>
    <TextInput
      id="email"
      name="email"
      onChange={[Function]}
      required={true}
      title="Email"
      type="email"
      value=""
    />
    <TextInput
      id="password"
      name="password"
      onChange={[Function]}
      required={true}
      title="Password"
      type="password"
      value=""
    />
    <div
      className="form-group my-4"
    >
      <styled.button
        className="btn btn-block"
        type="submit"
      >
        Sign In
      </styled.button>
    </div>
    <div
      className="dividerContainer"
    >
      <div
        className="divider"
      >
        <hr />
      </div>
      <p
        className="font-weight-bold text-muted"
      >
        OR
      </p>
      <div
        className="divider"
      >
        <hr />
      </div>
    </div>
    <div
      className="d-flex optionalLoginContainer"
    >
      <div
        className="optional-login"
      >
        <a
          href="https://anonymous-feedback-app.herokuapp.com/google"
        >
          <img
            alt="Sign up with google"
            src="google-button.png"
          />
        </a>
      </div>
    </div>
    <p>
      <Link
        to="/resetPassword"
      >
        Forgot Password?
      </Link>
    </p>
    <p>
      Don't have an account? Â 
      <Link
        to="/register"
      >
        <u>
          Sign Up
        </u>
      </Link>
    </p>
  </form>
</styled.div>
`;
